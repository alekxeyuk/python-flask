{
    "version": 2,
    "name": "python-flask",
    "builds": [
        {
            "src": "**/*.py",
            "use": "@now/python"
        },
        {
            "src": "static/**",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
          "src": "/image/(.*)",
          "status": 301,
          "headers": { "Location": "/image/$1" }
        }
      ]
}